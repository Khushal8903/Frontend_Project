{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": "off",
    "axe/forms": "off",
    "no-inline-styles": "off",
    "axe/parsing": "off",
    "css-prefix-order": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/text-alternatives": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 128",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ]
}